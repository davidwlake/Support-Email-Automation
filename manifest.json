 {
     "manifest_version": 2,

     "name": "Support Inbox Automation",
     "description": "Support Inbox Automation",
     "version": "1.0",

     "permissions": [
         "tabs", "<all_urls>", "notifications", "http://*/"
     ],
     
     "content_scripts": [
    {
        "matches": ["https://dealertrack-production.my.salesforce.com/_ui/common/data/LookupPage?lkfm=editPage*"],
        "js": ["contentScripts/selectUser.js"]
    },    
    {
        "matches": ["https://dealertrack-production.my.salesforce.com/_ui/common/data/LookupPage?lkpr=*"],
        "js": ["contentScripts/selectAccount.js"]
    },
    {
        "matches": ["https://dealertrack-production.my.salesforce.com/*/e?retURL=%*"],
        "js": ["contentScripts/editCase.js"]
    },
    {
        "matches": ["https://dealertrack-production.my.salesforce.com/*"],
            "js": ["contentScripts/runCase.js"]
    },
    {
        "matches": ["https://dealertrack-production.my.salesforce.com/*/a?retURL=*"],
            "js": ["contentScripts/assignCase.js"]
    },
    {
        "matches": ["https://dealertrack-production.my.salesforce.com/_ui/core/email/author/EmailAuthor?p*"],
            "js": ["contentScripts/writeEmail.js"]
    },
    {
        "matches": ["https://dealertrack-production.my.salesforce.com/500"],
            "js": ["contentScripts/listCase.js"]
    },
         {
        "matches": ["https://dealertrack-production.my.salesforce.com/_ui/search/ui/UnifiedSearchResults?searchType=2&sen=001&sen=003&sen=00T&sen=005&sen=00U&sen=500&sen=a6R&sen=00O&str=http%3A%2F%2F*&initialViewMode=detail&fen=001&collapse=1"],
            "js": ["contentScripts/w2cSearchURL.js"]
    },
    {
        "matches": ["https://dealertrack-production.my.salesforce.com/*?srPos=0&srKp=001"],
            "js": ["contentScripts/w2cGetSearchInfo.js"]
    }, 
    {
        "matches": ["https://dealertrack-production--c.na26.visual.force.com/apex/MLCSelectionPage*"],
            "js": ["contentScripts/category.js"]
    } 
         
    ],
     
     "browser_action": {
         "default_icon": "icon.png",
         "default_popup": "popup.html"
     },
     "background": {
         "scripts": ["background.js"],
         "persistent": false
     }
     
 }